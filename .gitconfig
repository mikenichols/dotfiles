[user]
  email = mykphyre@yahoo.com
  name = Mike Nichols

[alias]
  ap = add -p
  b = branch
  bb = bisect bad
  bg = bisect good
  bs = bisect start
  branch-name = rev-parse --abbrev-ref HEAD
  ci = commit
  cam = commit -am
  co = checkout
  com = checkout master
  cop2 = checkout @{-2}
  cop3 = checkout @{-3}
  cop4 = checkout @{-4}
  cop5 = checkout @{-5}
  cop6 = checkout @{-6}
  cp = cherry-pick
  cpx = cherry-pick -x
  d = diff
  dc = diff --cached
  ds = diff --cached
  f = fetch
  fp = fetch --prune
  it = !git init && git commit -m "root" --allow-empty
  kb = reset --hard HEAD~1
  l = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
  mm = merge master
  mmours = merge -s recursive -X ours master
  oops = commit --amend -C HEAD
  pirg = push --set-upstream origin HEAD
  phm = push heroku master
  poh = push --set-upstream origin HEAD
  pr = pull --rebase
  rb = reset --mixed HEAD~1
  r = rebase
  ra = rebase --abort
  rc = rebase --continue
  ri = rebase --interactive
  rbm = rebase --autosquash master
  rpo = remote prune origin
  s = status
  sa = stash apply
  sd = stash drop
  sp = stash pop
  st0 = stash --keep-index
  st1 = stash
  st2 = stash --include-untracked
  st3 = stash --all
  tc = commit -am temp_commit
  up = rev-parse --abbrev-ref --symbolic-full-name @{u}

[color]
  ui = auto

[push]
  default = simple

[rerere]
  enabled = true
  autoupdate = true

[rebase]
  autosquash = true
